{
  "name": "pastebin-api",
  "version": "5.1.6",
  "description": "A very simple npm package to interact with the pastebin api.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Dev-CasperTheGhost",
    "url": "https://www.caspertheghost.me"
  },
  "keywords": [
    "pastebin",
    "hastebin",
    "paste",
    "pastebin-api"
  ],
  "dependencies": {
    "fast-xml-parser": "^4.1.3",
    "undici": "^5.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api"
  },
  "bugs": {
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api/issues"
  },
  "homepage": "https://github.com/Dev-CasperTheGhost/pastebin-api#readme",
  "devDependencies": {
    "@casper124578/eslint-config": "^6.0.0",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2
  },
  "scripts": {
    "test": "ts-node ./tests/index.test.ts",
    "format": "prettier --write \"./**/**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "test-tsc": "tsc --noEmit",
    "build": "rimraf dist/ && tsup"
  }
}